@model Dictionary<string, string>

@{
    ViewBag.Title = "Index";
}


<div>
    <h4>View Data</h4>
    <hr />
    <table class="table table-striped">
        @foreach (var data in Model)
        {
            <tr>
                <td>
                    @data.Key
                </td>
                <td>
                    @data.Value
                </td>
            </tr>
        }
    </table>
</div>


